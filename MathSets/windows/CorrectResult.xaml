<Window x:Class="MathSets.windows.CorrectResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MathSets.windows"
        mc:Ignorable="d" Style="{StaticResource WindowMainStyle}" Icon="/resources/Icon.ico"
        Title="Результат выполнения задания" Height="650" Width="1000" MinHeight="650" MinWidth="1000">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Label Name="LBResult" Content="Результат верный. Ты молодец" Style="{StaticResource LabelHeaderResult}" Grid.Row="0"/>
        <Image Name="IMCorrectResult" Source="../resources/ResultCorrect.png" Grid.Row="1"/>
        <Button Name="BtnExit" Content="Закрыть" Grid.Row="2" Style="{StaticResource ButtonMainStyle}" HorizontalAlignment="Center" Click="BtnExit_Click" Cursor="Hand"/>
    </Grid>
</Window>
